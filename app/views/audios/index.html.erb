<body>
<p id="notice"><%= notice %></p>

<h1>Listing Audios</h1>

<table>
<%= link_to 'New Audio', new_audio_path %>

  <tbody>
    <% @audios.each do |audio| %>
      <tr>
        <td><iframe id="audio" width="500" height="110" src="<%= "https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/"+audio.url_audio %>"></td>
        <br>
        <td><%= link_to 'Show', audio %></td>
        <td><%= link_to 'Edit', edit_audio_path(audio) %></td>
        <td><%= link_to 'Destroy', audio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>

    <% end %>
  </tbody>
</table>
  <iframe width="100%" height="450" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/53450017&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe>
</body>



